{
    "name": "andrey-helldar/env-sync-symfony",
    "description": "env-sync package adapter for Laravel framework",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["env", "dotenv", "environment", "sync", "symfony", "framework"],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/env-sync/issues",
        "source": "https://github.com/andrey-helldar/env-sync"
    },
    "require": {
        "php": "^7.3|^8.0",
        "composer-plugin-api": "^2.0",
        "andrey-helldar/env-sync": "^2.0.1",
        "symfony/finder": "^4.0|^5.0"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.0|^9.0",
        "symfony/console": "^4.0|^5.0",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/phpunit-bridge": "^4.0|^5.0",
        "symfony/yaml": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\EnvSync\\Frameworks\\Symfony\\": ""
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "thanks": [
            {
                "name": "andrey-helldar/env-sync",
                "url": "https://github.com/andrey-helldar/env-sync"
            },
            {
                "name": "andrey-helldar/support",
                "url": "https://github.com/andrey-helldar/support"
            }
        ]
    }
}
